{
    "version": 2,
    "env": {
      "DB_USER": "@DB_USER",
      "DB_PASS": "@DB_PASS",
      "DB_HOST":"@DB_HOST",
    }
    ,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": [
          "GET",
          "POST",
          "PUT",
          "DELETE"
        ]
      }
    ]
  }
  